  "name": "Vinova Review Slide",
  "tag": "section",
  "settings": [
    {
      "type": "header",
      "content": "Title settings"
    },
    {
      "type": "text",
      "id": "title1",
      "label": "Title1",
      "default": "Title"
    },
    {
      "type": "color",
      "id": "title1_color",
      "label": "Titlte1 color",
      "default": "#fff"
    },
    {
      "type": "text",
      "id": "title2",
      "label": "Title2",
      "default": "Title"
    },
    {
      "type": "color",
      "id": "title2_color",
      "label": "Titlte2 color",
      "default": "#fff"
    },
    {
      "type": "text",
      "id": "title3",
      "label": "Title3",
      "default": "Title"
    },
    {
      "type": "color",
      "id": "title3_color",
      "label": "Titlte3 color",
      "default": "#fff"
    }
  ],
  "blocks": [
    {
      "type": "block_text",
      "name": "Block text",
      "settings": [
        {
          "type": "image_picker",
          "id": "image",
          "label": "Client photo"
        },
        {
          "type": "text",
          "id": "name",
          "label": "Client name"
        },
        {
          "type": "text",
          "id": "old",
          "label": "Client old"
        },
        {
          "type": "text",
          "id": "client",
          "label": "Client"
        },
        {
          "type": "text",
          "id": "date",
          "label": "Review date"
        },
        {
          "type": "textarea",
          "id": "description",
          "label": "Description"
        },
        {
          "type": "text",
          "id": "button_text",
          "label": "Button text"
        },
        {
          "type": "url",
          "id": "button_link",
          "label": "Button link"
        }
      ]
    }
  ],
  "presets": [
    {
      "name": "Vinova Review Slide"
    }
  ]
}
{% endschema %}

<!-- Initialize Swiper -->
<script>
  $(document).ready(function () {
    var swiper = new Swiper('.mySwiper', {
      slidesPerView: 3.2,
      grid: {
        rows: 2,
      },
      spaceBetween: 24,
      navigation: {
        nextEl: '.swiper-button-next',
        prevEl: '.swiper-button-prev',
      },
      pagination: {
        el: '.swiper-pagination',
        clickable: true,
      },
      breakpoints: {
        // when window width is >= 320
        320: {
          slidesPerView: 1,
          spaceBetween: 30,
        },
        // when window width is >= 1000
        1000: {
          slidesPerView: 2,
          spaceBetween: 30,
        },
        // when window width is >= 1200
        1200: {
          slidesPerView: 2.2,
          spaceBetween: 24,
        },
        // when window width is >= 1600
        1600: {
          slidesPerView: 3.2,
          spaceBetween: 24,
        },
      },
    });
  });
</script>

<!-- Swiper -->
{%- style -%}
  /* Title style css */
  #shopify-section-{{ section.id }} .heading1 {
    color: {{ section.settings.title1_color }};
  }
  #shopify-section-{{ section.id }} .heading2 {
    color: {{ section.settings.title2_color }};
  }
  #shopify-section-{{ section.id }} .heading3 {
    color: {{ section.settings.title3_color }};
    margin-top: 30px;
  }

  #shopify-section-{{ section.id }} .section-content {
    margin-top: 40px;
  }

  #shopify-section-{{ section.id }} .swiper-wrapper {
    max-height: 500px;
    margin-top: 40px;

    flex-wrap: wrap;
    flex-direction: column;
  }
  #shopify-section-{{ section.id }} .swiper {
    position: relative;
    width: 100%;
    height: 100%;
    margin-left: auto;
    margin-right: auto;
  }
  #shopify-section-{{ section.id }} .swiper:before {
    content: '';
    position: absolute;
    width: 150px;
    height: 555px;
    background: linear-gradient(90deg, #ffffff 0%, rgba(255, 255, 255, 0) 171.26%);
    z-index: 2;
  }
  #shopify-section-{{ section.id }} .swiper:after {
    content: '';
    position: absolute;
    width: 150px;
    height: 500px;
    background: linear-gradient(90deg, #ffffff 0%, rgba(255, 255, 255, 0) 171.26%);
    transform: rotate(-180deg);
    z-index: 2;
    right: 0;
    top: 0;
  }
  #shopify-section-{{ section.id }} .swiper-button-next {
    position: absolute;
    right: 170px;
  }
  #shopify-section-{{ section.id }} .swiper-button-prev {
    position: absolute;
    left: 170px;
  }
  #shopify-section-{{ section.id }} .swiper-button-prev svg,
  #shopify-section-{{ section.id }} .swiper-button-next svg
  {
    scale: 2;
  }
  #shopify-section-{{ section.id }} .swiper-button-prev:after,
  #shopify-section-{{ section.id }} .swiper-button-next:after
  {
    content: '';
  }
  #shopify-section-{{ section.id }} .swiper-slide {
    text-align: center;
    font-size: 18px;
    background: #fff;
    height: calc((100% - 30px) / 2) !important;

    /* Center slide text vertically */
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    color: #5E5E5E;

    /* white */
    background: #FFFFFF;
    /* gray30 */
    border: 1px solid #ECECEC;
    /* dark_shadow */
    box-shadow: 0px 10px 20px rgba(211, 211, 211, 0.25);
    border-radius: 15px;
    padding: 17px;
  }
  #shopify-section-{{ section.id }} .swiper-slide img {
    height: 100%;
    width: 100%;
    border-radius: 50%;
  }
  #shopify-section-{{ section.id }} .swiper-slide.first-row {
    margin-left: calc(-100% / 7) !important;
  }

  /* Slide components style css */
  #shopify-section-{{ section.id }} .swiper-slide .header {
    display: flex;
    justify-content: start;
    width: 100%;
    align-items: center;
  }
  #shopify-section-{{ section.id }} .swiper-slide .title-box {
    width: 100%;
    margin-left: 14px;
  }
  #shopify-section-{{ section.id }} .swiper-slide .title-box .review-info {
    display: flex;
    justify-content: space-between;
    margin-top: 9px;
  }
  #shopify-section-{{ section.id }} .swiper-slide .title-box .client-name {
    display: flex;
  }
  #shopify-section-{{ section.id }} .swiper-slide .body {
    margin-top: 12px;
    text-align: left;
  }

  #shopify-section-{{ section.id }} .swiper-slide .name {
    color: #3F4D6A;
  }
  #shopify-section-{{ section.id }} .swiper-slide .old {
    color: #000000;
  }
  #shopify-section-{{ section.id }} .swiper-slide .description {
    line-height: 24px;
  }
  #shopify-section-{{ section.id }} .swiper-slide .btn-show-more {
    color: #EA7503;
  }

  @media(max-width: 1200px) {
    #shopify-section-{{ section.id }} .swiper:after,
    #shopify-section-{{ section.id }} .swiper:before {
      width: 50px;
    }
    #shopify-section-{{ section.id }} .swiper-button-next {
      right: 70px;
    }
    #shopify-section-{{ section.id }} .swiper-button-prev {
      left: 70px;
    }
  }
  @media(max-width: 999px) {
    #shopify-section-{{ section.id }} .swiper:after,
    #shopify-section-{{ section.id }} .swiper:before {
      width: 0px;
    }
    #shopify-section-{{ section.id }} .swiper-button-next {
      right: 30px;
    }
    #shopify-section-{{ section.id }} .swiper-button-prev {
      left: 30px;
    }
    #shopify-section-{{ section.id }} .swiper-slide,
    #shopify-section-{{ section.id }} .swiper-slide.first-row
    {
      margin-left: 0 !important;
    }
  }
{%- endstyle -%}
<div class="section-content">
  <div class="container main-content">
    <div class="title-box">
      {% if section.settings.title1 != blank %}
        <div class="heading1 h2 font-700 text-center">{{ section.settings.title1 }}</div>
      {% endif %}
      {% if section.settings.title2 != blank %}
        <div class="heading2 h3 font-600 text-center">{{ section.settings.title2 }}</div>
      {% endif %}
      {% if section.settings.title3 != blank %}
        <div class="heading3 h3 font-600 text-center">{{ section.settings.title3 }}</div>
      {% endif %}
    </div>
  </div>

  <div class="swiper mySwiper">
    <div class="swiper-wrapper">
      {% if section.blocks.size > 0 %}
        {% for block in section.blocks %}
          {% assign mo = forloop.index | modulo: 2 %}
          <div
            class="swiper-slide {% if mo == 0 %} {{ 'first-row' }} {% else %} {{ 'second-row' }} {% endif %}"
            style="margin-left: -150px;"
          >
            <div class="header">
              <div class="client-photo">
                {%- if block.settings.image != blank -%}
                  <img src="{{ block.settings.image | image_url: width: 80 }}">
                {%- endif -%}
              </div>
              <div class="title-box">
                <div class="client-name">
                  {% if block.settings.name != blank %}
                    <p class="h4 font-700 name">{{ block.settings.name }}</p>
                  {% endif %}
                  {% if block.settings.old != blank %}
                    <p class="h4 font-700 old">{{ block.settings.old }}</p>
                  {% endif %}
                </div>
                <div class="review-info">
                  {% if block.settings.client != blank %}
                    <p class="h6 font-400">{{ block.settings.client }}</p>
                  {% endif %}
                  {% if block.settings.date != blank %}
                    <p class="h6 font-400">
                      <svg width="20" height="22" viewBox="0 0 20 22" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M1.09253 8.40421H18.9165" stroke="#5E5E5E" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                        <path d="M14.442 12.3097H14.4512" stroke="#5E5E5E" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                        <path d="M10.0047 12.3097H10.014" stroke="#5E5E5E" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                        <path d="M5.55793 12.3097H5.5672" stroke="#5E5E5E" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                        <path d="M14.442 16.1962H14.4512" stroke="#5E5E5E" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                        <path d="M10.0047 16.1962H10.014" stroke="#5E5E5E" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                        <path d="M5.55793 16.1962H5.5672" stroke="#5E5E5E" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                        <path d="M14.0438 1V4.29078" stroke="#5E5E5E" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                        <path d="M5.9654 1V4.29078" stroke="#5E5E5E" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                        <path fill-rule="evenodd" clip-rule="evenodd" d="M14.2383 2.57922H5.77096C2.83427 2.57922 1 4.21516 1 7.22225V16.2719C1 19.3263 2.83427 21 5.77096 21H14.229C17.175 21 19 19.3546 19 16.3475V7.22225C19.0092 4.21516 17.1842 2.57922 14.2383 2.57922Z" stroke="#5E5E5E" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                      </svg>
                      {{ block.settings.date }}
                    </p>
                  {% endif %}
                </div>
              </div>
            </div>

            <div class="body">
              {%- if block.settings.description != blank -%}
                <p class="description h6 font-400">
                  {{
                    block.settings.description
                    | truncate: 290
                    | append: ' <a href="'
                    | append: block.settings.button_link
                    | append: '" class="h6 font-400 btn-show-more">'
                    | append: block.settings.button_text
                    | append: '</a>'
                  }}
                </p>
              {%- endif -%}
            </div>
          </div>
        {% endfor %}
      {% endif %}
    </div>

    <div class="swiper-button-next">
      <svg width="42" height="42" viewBox="0 0 42 42" fill="none" xmlns="http://www.w3.org/2000/svg">

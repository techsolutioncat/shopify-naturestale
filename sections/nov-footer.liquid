{%- style -%}
  #shopify-section-nov-footer .title_block .title {
    color: {{ section.settings.contact_title_color }};
  }
  #shopify-section-nov-footer .title_block .sub_title {
    color: {{ section.settings.contact_subtitle_color }};
  }
{%- endstyle -%}

<footer>
  <div class="contact-from-wrap container">
    {% if section.settings.title_contact != blank %}
      <div class="title_block text-center">
        <p class="font-700 h2 title">{{ section.settings.title_contact }}</p>
        {% if section.settings.sub_title_contact != blank %}
        <div class="sub_title font-600 h5">
          {{ section.settings.sub_title_contact }}
        </div>
        {% endif %}
      </div>
    {% endif %}
    {% if section.settings.contactform %}
      <div class="container">
        <div class="footer-contactform">
          <div class="container">
            {% form 'contact' %}
              {{ form.errors | default_errors }}
          
              <div class="first-name">
                <input type="text" name="contact[first_name]" id="first-name" placeholder="Your name*" required>
              </div>
          
              <div class="last-name">
                <input type="text" name="contact[last_name]" id="last-name" placeholder="Your Last name*" required>
              </div>
          
              <div class="email">
                <input type="email" name="contact[email]" id="email" placeholder="Email*" required>
              </div>
          
              <div class="submit">
                <button type="submit" class="font-700 text-uppercase font-700 site-button button_S btn-submit">{{ 'contact.form.submit' | t }}</button>
              </div>
            {% endform %}
          </div>
        </div>
      </div>
    {% endif %}
  </div>
  <div class="footer-layout pt-80 pb-30">
    <div class="container">
      {% if section.settings.newsletter == true %}
        <div class="block_newsletter footer_newsletter">
          {% if section.settings.newsletter_title != blank %}
            <div class="title_section text-center">
              <div class="title"><strong>{{ section.settings.newsletter_title }}</strong></div>
              {% if section.settings.newsletter_des != blank %}
                <div class="sub_title">{{ section.settings.newsletter_des }}</div>
              {% endif %}
            </div>
          {% endif %}
          {% form 'customer' %}
            {{ form.errors | default_errors }}
            {% if form.posted_successfully? %}
              <p class="form--success">{{ 'general.newsletter_form.confirmation' | t }}</p>
            {% else %}
              <div class="input-group">
                <input type="hidden" name="contact[tags]" value="newsletter">
                <input type="email"
                  name="contact[email]"
                  id="Email_Footer"
                  class="input-group__field newsletter__input form-control"
                  value="{% if customer %}{{ customer.email }}{% endif %}"
                  placeholder="{{ 'general.newsletter_form.email_newsletter' | t }}">
                  <button type="submit" class="btn newsletter__submit" name="commit">
                    {{ 'general.newsletter_form.submit' | t }}
                  </button>
              </div>
            {% endif %}
          {% endform %}
        </div>
      {% endif %}
      {% if section.blocks.size > 0 %}
        <div class="row pt-45">
          {% for block in section.blocks %}
            {% if block.type == 'block_contact' %}
              <div class="footer_contact block_footer{% if block.settings.block_title != blank %} mt-xs-0 mb-xs-0{% endif %} {{ block.settings.grid_xl }} {{ block.settings.grid_lg }} {{ block.settings.grid_md }} {{ block.settings.grid_sm }}{% if block.settings.block_class != blank %} {{ block.settings.block_class }}{% endif %}">
                <div class="block">
                  {% if block.settings.block_title != blank %}
                  <div class="title-block h5">
                    {{ block.settings.block_title }}
                    <span class="f_btn_sl d-sm-none"><i class="zmdi zmdi-plus"></i></span>
                  </div>
                  {% endif %}
                  <div class="block-content{% if block.settings.block_title != blank %} h_t pb-xs-30{% endif %}">
                    {% if block.settings.contact_address != blank %}
                    <div class="contact d-flex align-items-center">
                      <div class="icon-contact">
                        <i class="zmdi zmdi-pin"></i>
                      </div>
                      <div class="address-content">
                        <div class="content-address">{{ block.settings.contact_address }}</div>
                      </div>
                    </div>
                    {% endif %}
                    {% if block.settings.contact_phone != blank %}
                    <div class="contact d-flex align-items-center">
                      <div class="icon-contact">
                        <i class="zmdi zmdi-phone-in-talk"></i>
                      </div>
                      <div class="phone-content">
                        <div class="number">{{ block.settings.contact_phone }}</br>{{ block.settings.contact_phone_2 }}</div>
                      </div>
                    </div>
                    {% endif %}
                    {% if block.settings.contact_email != blank %}
                    <div class="contact d-flex align-items-center">
                      <div class="icon-contact">
                        <i class="zmdi zmdi-email"></i>
                      </div>
                      <div class="email-content">
                        <div class="email">{{ block.settings.contact_email }}</br>{{ block.settings.contact_email_2 }}</div>
                      </div>
                    </div>
                    {% endif %}
                    {% if block.settings.img != blank %}
                      <img class="lazyload mt-40" data-src="{{ block.settings.img | img_url: '300x' }}">
                    {% endif %}
                  </div>
                </div>
              </div>
            {% endif %}

            {% if block.type == 'block_menu' %}
              {%- assign footer_linklist = block.settings.menu_handle -%}
              <div class="footer_menu block_footer{% if block.settings.block_title != blank %} mt-xs-0 mb-xs-0{% endif %} {{ block.settings.grid_xl }} {{ block.settings.grid_lg }} {{ block.settings.grid_md }} {{ block.settings.grid_sm }}{% if block.settings.block_class != blank %} {{ block.settings.block_class }}{% endif %}">
                {% if block.settings.block_title != blank %}
                <div class="title-block h5">
                  {{ block.settings.block_title }}
                  <span class="f_btn_sl d-sm-none"><i class="zmdi zmdi-plus"></i></span>
                </div>
                {% endif %}
                <div class="block-content{% if block.settings.block_title != blank %} h_t pb-xs-30{% endif %}">
                  {% if linklists[footer_linklist].links.size > 0 %}
                    <ul class="site-footer__linklist list-unstyled">
                      {% for link in linklists[footer_linklist].links %}
                        <li class="site-footer__linklist-item">
                          <a class="font-400 h6" href="{{ link.url }}">{{ link.title }}</a>
                        </li>
                      {% endfor %}
                    </ul>
                  {% endif %}
                </div>
              </div>
            {% endif %}

            {% if block.type == 'block_html' %}
              <div class="footer_html block_footer{% if block.settings.block_title != blank %} mt-xs-0 mb-xs-0{% endif %} {{ block.settings.grid_xl }} {{ block.settings.grid_lg }} {{ block.settings.grid_md }} {{ block.settings.grid_sm }}{% if block.settings.block_class != blank %} {{ block.settings.block_class }}{% endif %}">
                <div class="block">
                  {% if block.settings.block_title != blank %}
                  <div class="title-block h5">
                    {{ block.settings.block_title }}
                    <span class="f_btn_sl d-sm-none"><i class="zmdi zmdi-plus"></i></span>
                  </div>
                  {% endif %}
                  <div class="block-content{% if block.settings.block_title != blank %} h_t pb-xs-30{% endif %}">
                    {% if block.settings.img != blank %}
                    <img src="{{ block.settings.img | img_url: '190x' }}">
                    {% endif %}
                    {% if block.settings.html_content != blank %}
                      <div class="bl_html mt-20">{{ block.settings.html_content }}</div>
                    {% endif %}
                    {% if block.settings.social %}
                      <div class="d-flex align-items-center mt-30">
                        {% include 'nov-social' %}
                      </div>
                    {% endif %}
                  </div>
                </div>
              </div>
            {% endif %}

            {% if block.type == 'block_newsletter' %}
              <div class="footer_newsletter block_footer{% if block.settings.block_title != blank %} mt-xs-0 mb-xs-0{% endif %} {{ block.settings.grid_xl }} {{ block.settings.grid_lg }} {{ block.settings.grid_md }} {{ block.settings.grid_sm }}{% if block.settings.block_class != blank %} {{ block.settings.block_class }}{% endif %}">
                {% if block.settings.block_title != blank %}
                <div class="title-block h5">
                  {{ block.settings.block_title }}
                  <span class="f_btn_sl d-sm-none"><i class="zmdi zmdi-plus"></i></span>
                </div>
                {% endif %}
                <div class="block-content{% if block.settings.block_title != blank %} h_t pb-xs-30{% endif %}">
                  {% if block.settings.newsletter_des != blank %}
                    <div class="des mb-25">{{ block.settings.newsletter_des }}</div>
                  {% endif %}
                  <div class="block-content">
                    {% form 'customer' %}
                      {{ form.errors | default_errors }}
                      {% if form.posted_successfully? %}
                        <p class="form--success">{{ 'general.newsletter_form.confirmation' | t }}</p>
                      {% else %}
                        <div class="input-group">
                          <input type="hidden" name="contact[tags]" value="newsletter">
                          <input type="email"
                            name="contact[email]"
                            id="Email_Footer"
                            class="input-group__field newsletter__input form-control"
                            value="{% if customer %}{{ customer.email }}{% endif %}"
                            placeholder="{{ 'general.newsletter_form.email_newsletter' | t }}">
                          <div class="input-group__btn">
                            <button type="submit" class="btn newsletter__submit" name="commit" id="Subscribe_footer">
                              <span class="newsletter__submit-text--large">{{ 'general.newsletter_form.submit' | t }}</span>
                            </button>
                          </div>
                        </div>
                      {% endif %}
                    {% endform %}
                  </div>
                  {% if block.settings.social %}
                    <div class="d-flex align-items-center mt-30">
                      <span class="title_social text-uppercase font-700">{{ 'layout.footer.title_social' | t }} : </span>
                      {% include 'nov-social' %}
                    </div>
                  {% endif %}
                </div>
              </div>
            {% endif %}

            {% if block.type == 'block_image' %}
              <div class="footer_image block_footer{% if block.settings.block_title != blank %} mt-xs-0 mb-xs-0{% endif %} {{ block.settings.grid_xl }} {{ block.settings.grid_lg }} {{ block.settings.grid_md }} {{ block.settings.grid_sm }}{% if block.settings.block_class != blank %} {{ block.settings.block_class }}{% endif %}">
                <div class="block">
                  {% if block.settings.block_title != blank %}
                  <div class="title-block h5">
                    {{ block.settings.block_title }}
                    <span class="f_btn_sl d-sm-none"><i class="zmdi zmdi-plus"></i></span>
                  </div>
                  {% endif %}
                  <div class="block-content{% if block.settings.block_title != blank %} h_t pb-xs-30{% endif %}">
                    {% if block.settings.img != blank %}
                      <img class="img-fluid lazyload " data-src="{{ block.settings.img | img_url: '300x' }}">
                    {% elsif block.settings.img2 != blank %}
                      <img class="img-fluid lazyload mt-45" data-src="{{ block.settings.img2 | img_url: '300x' }}">
                    {% else %}
                    <svg width="89" height="110" viewBox="0 0 89 110" fill="none" xmlns="http://www.w3.org/2000/svg">
                      <path d="M6.88315 78.8157H10.2845V93.7129H6.88315L3.40132 86.5289V93.7129H0V78.8157H3.40132H3.42145L6.88315 86.305V78.8157Z" fill="white"/>

{%- assign bg_image = section.settings.background_image -%}
<style>
  .footer-layout {
    {% if section.settings.background_color != blank %}
      background-color: {{ section.settings.background_color }};
    {% endif %}
    {% if bg_image.src != blank %}
      background-image: url("{{ bg_image | img_url: '1920x' }}");
      background-repeat: no-repeat;
    {% endif %}
  }
  .nov-footer, .site-footer__linklist-item a {
    color: {{ section.settings.color }};
  }
  .footer-layout .title-block,
  .footer-layout .title,
  .footer_newsletter input,
  .footer_newsletter .title_social,
  .footer_newsletter .form-control:focus {
    color: {{ section.settings.color_t }};
  }
  .footer_menu li a:hover {
    color: var(--theme-color-default);
  }
  .nov-footer .block_social a {
    color: {{ section.settings.c_social }};
    background-color: {{ section.settings.bg_social }};
  }
  {% if section.settings.bg_copy != blank %}
    .footer_copyright {
      {% comment %} background-color: {{ section.settings.bg_copy }}; {% endcomment %}
      background-image: url("{{ section.settings.bg_copy_image | img_url: '1920x' }}"); background-repeat: no-repeat; background-size: cover;background-position: center;
    }
  {% endif %}
  {% if section.settings.bg_contact_form != blank %}
    .footer-contactform {
      background-image: url("{{ section.settings.bg_contact_form | img_url: '1920x' }}"); background-repeat: no-repeat; background-size: cover;background-position: center;
    }
  {% endif %}
</style>